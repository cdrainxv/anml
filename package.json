{
  "name": "anml",
  "version": "0.1.0",
  "description": "scientific and common names of animals library",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "eslint-check":
      "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "prettier-eslint src/**/*.js --write",
    "test": "nyc mocha src/index.test.js",
    "test:watch": "npm t -- --watch",
    "validate": "npm-run-all --parallel lint test build",
    "precommit": "npm run validate"
  },
  "keywords": ["animals", "scientic", "common", "names"],
  "files": ["dist"],
  "dependencies": {
    "unique-random": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-istanbul": "4.1.5",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chai-fuzzy": "1.6.1",
    "eslint": "4.12.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.3.1",
    "husky": "0.14.3",
    "mocha": "4.0.1",
    "npm-run-all": "4.1.2",
    "nyc": "11.3.0",
    "prettier": "1.8.2",
    "prettier-eslint": "8.2.2",
    "prettier-eslint-cli": "4.4.0",
    "rimraf": "2.6.2"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "reporter": ["text", "lcov"],
    "instrument": false,
    "sourceMap": false,
    "require": ["babel-register"],
    "include": ["src"]
  },
  "babel": {
    "presets": ["env"],
    "plugins": ["babel-plugin-istanbul"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdrainxv/anml.git"
  },
  "author": "cdrani (https://github.com/cdrainxv)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cdrainxv/anml/issues"
  },
  "homepage": "https://github.com/cdrainxv/anml#readme"
}
